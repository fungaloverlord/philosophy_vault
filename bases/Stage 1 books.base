views:
  - type: table
    name: Table
    filters:
      and:
        - type == "literature-note"
        - stage == 1
    order:
      - file.name
      - status
      - difficulty
      - branch
    sort:
      - property: status
        direction: ASC
    columnSize:
      file.name: 317
      note.status: 87
      note.difficulty: 94
      note.related: 611
